<!DOCTYPE html>
<html>
<head>
	<title>Students Report</title>
	<link rel="icon" type="image/gif/png" href="/images/skllogo.png" />
    <link href="/css/bootstrap.min.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="/stylesheets/student-report.css" media="screen,projection">

	<link rel="stylesheet" type="text/css" href="/stylesheets/print-std-report.css" media="print">
</head>
<body>
<div class="printing">
	<h4>Preparing Your PDF</h4>
	<img src="/images/workgreen.gif" alt="">
</div>
<div id="border">
<div id="pagination" class="row">
    <div class="col">
        <%-pages%>
    </div>
    <div class="col">
        <button id="printbtn" class="btn btn-light" onclick="window.location.href='/print/<%-pageCount%>'">
            <img src="/images/print.png" alt="" width="20" height="20"> Print
        </button>
        <br>
        <a href="/report/daily1/<%=data.std%>/<%=data.sec%>/1/page1/1/">Right Page Attnd</a>
    </div>
</div>
	<div class="main">
        <br>
		<div id="loop">
			<div class="wid">
				<div class="aone">
					<img src="/images/donbosco.jpg" width="90px" height="90px">
				</div>
				<div class="atwo">
					<div>DON BOSCO MATRIC HR. SEC. SCHOOL - STUDENTS PARTICULARS</div>
					<br>
					<div>
						<span>CLASS <%=data.std%> - <%=data.sec%></span>
						<span>YEAR <%=data.year%>-<%=(parseInt(data.year)+1)%></span>
					</div>
				</div>
			</div>
		<br>
		<hr class="style-seven">

		<%Object.keys(data.items).forEach(function (key) {%>
		<div class="main-content">
			<br>
			<div class="one">
				<img width="120px" height="170px" src="/uploads/<%=data.items[key].img%>.jpg" >
			</div>
			<div class="two">
				<table>
					<tbody>
						<tr>
							<th class="text-center">S.NO</th>
							<th><%=(data.start+1)%></th>
							<th colspan="3" class="text-center"><%=data.items[key].name%></th>
						</tr>
						<tr>
							<th class="text-center">Ad. NO</th>
							<th><%=data.items[key].add_no%></th>
							<td colspan="2"> <strong>FATHER :</strong>  <span class="text-center"><%=data.items[key].fname%></span></td>
							<td><span class="ans"><%=data.items[key].foc%></span></td>
						</tr>
						<tr>
							<th class="text-center">Ex. NO</th>
							<th><%=data.items[key].exno%></th>
							<td colspan="2"><strong>MOTHER :</strong> <span class="text-center"><%=data.items[key].mname%></span> </td>
							<td> <span class="ans"><%=data.items[key].moc%></span></td>
						</tr>
						<tr>
							<th class="text-center">GENDER</th>
							<th><%=data.items[key].gender%></th>
							<td><strong>Date of Birth:</strong>  <span class="ans"><%=data.items[key].dob%></span> </td>
							<td><strong>Community:</strong>  <span class="ans"><%=data.items[key].community%></span> </td>
							<td rowspan="3" class="address-col">
								<strong>ADDRESS:</strong><br>
								<%=data.items[key].address%>
							</td>
						</tr>
						<tr>
							<th class="text-center">BG</th>
							<th><%=data.items[key].bg%></th>
							<td> <strong>Religion :</strong><span class="ans"><%=data.items[key].religion%></span></td>
							<td><strong>Caste :</strong> <span class="ans"><%=data.items[key].caste%></span></td>
						</tr>
						<tr align="center">
							<td colspan="2"><strong>EMIS NO</strong></td>
							<td><%=data.items[key].emis%></td>
							<td>
								<strong>PH.No:</strong>
								<p><%=data.items[key].mob1%>
								<br>
								<%=data.items[key].mob2%></p>
							</td>
						</tr>
					</tbody>
				</table>
			</div>
			<hr id="lasthr" class="style-one">
		</div>
		<%data.start++});%>
		<%if(last==="ok"){%>
			<table class="report">
				<tbody>
			<tr>
				<td>class</td>
				<td>STRENGTH</td>
				<td>CHRISTIAN</td>
				<td>HINDU</td>
				<td>MUSLIM</td>
				<td>SC(A)</td>
				<td>SC</td>
				<td>ST</td>
				<td>BC</td>
				<td>BCM</td>
				<td>MBC</td>
				<td>OC</td>
				<td>FC</td>
				<td>OTHERS</td>
			</tr>
			<tr>
				<td><%=std%>-<%=sec%></td>
				<td>
					TOTAL:<%=data.pageCount%>
					<br>
					BOYS:<%=data.male%>
					<br>
					GIRLS:<%=data.female%>
				</td>
				<td><%=data.christian%></td>
				<td><%=data.hindu%></td>
				<td><%=data.muslim%></td>
				<td><%=data.sca%></td>
				<td><%=data.sc%></td>
				<td><%=data.st%></td>
				<td><%=data.bc%></td>
				<td><%=data.bcm%></td>
				<td><%=data.mbc%></td>
				<td><%=data.oc%></td>
				<td><%=data.fc%></td>
				<td><%=data.others%></td>
			</tr>
		</tbody>
			</table>
			<%}%>
	</div>
	</div>
<script src="/javascripts/jquery.js"></script>
<script>
// $(".printing").css("display","none");

$("#printbtn").click(function(){
	$("#border").css("display","none");
	$(".printing").css("display","block");
	window.location.href='/report/print/<%-pageCount%>/<%=data.std%>/<%=data.sec%>';
});
</script>
</body>
</html>